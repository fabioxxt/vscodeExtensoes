{
    "name": "vsmysql",
    "displayName": "vsmysql",
    "description": "Terminal de controle das databases do mysql.",
    "version": "0.0.1",
    "publisher": "vsmysql",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:vsMysqlNodes"
    ],
    "main": "./extension",
    "contributes": {
        "views": {
            "explorer": [
                {
                    "id": "vsMysqlNodes",
                    "name": "VS MYSQL"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "vsMysqlNodes.refresh",
                    "when": "view == vsMysqlNodes",
                    "group": "navigation"
                }
            ]
        },
        "commands": [
            {
                "command": "vsMysqlNodes.refresh",
                "title": "Refresh"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "mysql": "^2.15.0"
    }
}
